<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>Ruya</id>
    <version>0.0.0.1</version>
    <authors>Cengiz Ilerler</authors>
    <owners>cilerler</owners>
    <licenseUrl>https://raw.githubusercontent.com/cilerler/Ruya/master/LICENSE.txt</licenseUrl>
    <projectUrl>https://github.com/cilerler/Ruya</projectUrl>
    <iconUrl>http://ICON_URL_HERE_OR_DELETE_THIS_LINE</iconUrl>
    <requireLicenseAcceptance>true</requireLicenseAcceptance>
    <description>Ruya is a .Net Framework wrapper</description>
    <releaseNotes>Summary of changes made in this release of the package.</releaseNotes>
    <copyright>Copyright 2015</copyright>
    <tags>Tag1 Tag2</tags>
    <frameworkAssemblies>
      <frameworkAssembly assemblyName="System.Web" targetFramework="net40" />
      <frameworkAssembly assemblyName="System.Net" targetFramework="net40-client, net40" />
      <frameworkAssembly assemblyName="Microsoft.Devices.Sensors" targetFramework="sl4-wp" />
      <frameworkAssembly assemblyName="System.Json" targetFramework="sl3" />
      <frameworkAssembly assemblyName="System.ComponentModel.DataAnnotations" />
    </frameworkAssemblies>
  </metadata>
  <dependencies>
    <dependency id="CommandLineMenu" version="1.0.0" />
    <dependency id="EntityFramework" version="6.1.3" />
    <dependency id="Microsoft.AspNet.Mvc" version="5.2.3" />
    <dependency id="Microsoft.AspNet.SignalR.Owin" version="1.2.2" />
    <dependency id="Microsoft.AspNet.SignalR.SystemWeb" version="2.2.0" />
    <dependency id="Microsoft.AspNet.WebApi" version="5.2.3" />
    <dependency id="Microsoft.AspNet.WebApi.Owin" version="5.2.3" />
    <dependency id="Microsoft.Owin.Cors" version="3.0.1" />
    <dependency id="Microsoft.Owin.Host.HttpListener" version="3.0.1" />
    <dependency id="Microsoft.Owin.Hosting" version="3.0.1" />
    <dependency id="Microsoft.Owin.StaticFiles" version="3.0.1" />
    <dependency id="WebActivatorEx" version="2.0.6" />
  </dependencies>
  <files>
    <file src="lib\net45\Ruya.Core.dll" target="lib\net45\Ruya.Core.dll" />
    <file src="bin\Debug\**\*.*" target="lib\" exclude="**\*.pdb" />
  </files>
</package>

